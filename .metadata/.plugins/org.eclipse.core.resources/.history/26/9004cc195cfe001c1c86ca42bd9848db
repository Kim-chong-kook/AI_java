package day03;

public class Test1 {

	public static void main(String[] args) {
		int num=5;
		int result = ++num; // num = num+1
		System.out.println(result);
		System.out.println(num);
		
		int num1=5;
		int result2 = num1++; // num = num+1
		System.out.println(result2);
		System.out.println(num1);
	}

}
