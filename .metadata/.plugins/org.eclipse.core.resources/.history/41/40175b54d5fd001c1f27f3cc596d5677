package day02_test;

import java.util.Scanner;

public class Test9 {
	public static void main(String[] args) {
		Scanner sc  = new Scanner(System.in);
		double x = 0, y = 0;
		System.out.println("두 개의 실수를 입력하세요.");
		x = sc.nextDouble();
		y = sc.nextDouble();
		
		System.out.printf("x = %.2f \n",x);
		System.out.printf("y = %.2f \n",y);
		
	}
}
