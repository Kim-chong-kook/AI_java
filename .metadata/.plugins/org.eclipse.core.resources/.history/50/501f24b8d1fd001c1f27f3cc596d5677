package day02_test;
import java.util.Scanner;

public class Test7 {
	public static void main(String[] args) {
		int a=0; int b = 0;
		Scanner sc  = new Scanner(System.in);
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.printf("%d + %d = %d \n",a,b,a+b);
		System.out.printf("%d * %d = %d \n",a,b,a*b);
	}
}
