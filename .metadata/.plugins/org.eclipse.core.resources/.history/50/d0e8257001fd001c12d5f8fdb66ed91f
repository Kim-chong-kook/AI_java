package kr.co.tj;

public class BinaryTest {
	public static void main(String[] args) {
		int num  = 10;
		int bNum = 0B1010;
		int oNum = 012;
		int xNum = 0XA;
		
		System.out.println(num);
		System.out.println(bNum);
		System.out.println(oNum);
		System.out.println(xNum);
		
		byte b = 1000; // 1byte의 범위인 -128~127의 범위를 넘어 에러상태.
	}

}
