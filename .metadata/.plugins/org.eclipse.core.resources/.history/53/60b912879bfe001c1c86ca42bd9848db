package day03;

public class Test10 {
	public static void main(String[] args) {
		int a = 20;
		int b = 60;
		
		if (a < 10 && b<50) {
			System.out.println("작진 않다.");
		}
		if (a < 15 && b<55) {
			System.out.println("그렇다.");
		}
		else if (a>15 && b>55) {
			System.out.println("그리 큰수는 아니다.");
		}
	}
}
