package kr.co.tj;

public class Testing {

	public static void main(String[] args) {
		int a =0; int b=0; int c=0; int d=0; int e=0;
		a = 10; b = 5; c=7;d=9;e=10;
		int i= 0;
		for(i=0;i<1;i++) {
			a += 3;
			b -= 3;
			c *= 3;
			d /= 3;
			e %= 3;
		}
		System.out.print("%d,%d,%d,%d,%d",a + b + c + d + e);
	}

}
