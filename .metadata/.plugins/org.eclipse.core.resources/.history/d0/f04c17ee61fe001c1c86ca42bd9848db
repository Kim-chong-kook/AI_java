package day03;

public class RealtionalTest {
	public static void main(String[] args) {
		int num1 = 5;
		int num2 = 3;
		
		boolean value = (num1>num2);
		System.out.println(value);
		
		System.out.println(num1 < num2);
		System.out.println(num1 >= num2);
		System.out.println(num1 <= num2);
		System.out.println(num1 == num2);
		System.out.println(num1 != num2); // (5는 3이 아니다) => true.

	}
}
