package kr.co.tj;

public class ForTest { // 1~10 숫자 더하기.

	public static void main(String[] args) {
		int count=1;
		int sum=0;
		
		for(int i=0;i<10;i++,count++) { // 10번
			sum += count;
			//count++; // 위에 쓰든지 한번 사용한다.
		}
		/* System.out.println("for문 결과 = " + sum);
		
		int num=1;
		int total = 0;
		
		while(num<=10) {
			total +=num;
			num++;
			
		}
		System.out.println("while문 결과 = " + total);
		*/	
				
	}

}
