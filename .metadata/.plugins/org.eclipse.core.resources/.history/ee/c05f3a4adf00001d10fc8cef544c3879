package kr.co.tj;

import java.util.Scanner;

public class Testing14 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a=0,b=0,c=0,d=0;
		System.out.println("세가지 수를 입력하세요.");
		a = scan.nextInt();
		b = scan.nextInt();
		c = scan.nextInt();
		
		if(a<b) {
			d = b;
			b = a;
			a = d;
			
			if(b<c) {
				d = b;
				b = c;
				c = d;
			}
		}
		System.out.printf("입력받은 수중 가장 큰 수는 %d 입니다.\n",a);
		
	}

}
