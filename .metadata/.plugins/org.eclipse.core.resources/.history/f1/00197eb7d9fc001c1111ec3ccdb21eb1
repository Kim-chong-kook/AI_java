package kr.co.tj;

public class Test {
	public static void main(String[] args) {
		// 폰트설정 : windows / preference / font 검색.. text font에서 변경.
		// main 등 용어 쓰고 : 컨트롤 + space로 자동생성
		// 블럭 & 컨트롤 + 쉬프트 + F : 자동정렬
		// 블럭 & 컨트롤 + 알트 + 하방키 : 복사
		// 컨트롤 + F11 : 실행
		System.out.println("java hello1");
		System.out.println("java hello2");
		System.out.println("java hello3");
		System.out.println("java hello4");
		System.out.println("java hello5");
		System.out.println("java hello6");
		System.out.println("java hello7");
		System.out.println("java hello");

	}
}
